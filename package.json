{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,ts,tsx}' --fix",
    "generate": "run-s generate:*",
    "generate:codegen": "graphql-codegen",
    "ic:deploy": "pnpm ic:counter && pnpm ic:generate:counter",
    "ic:counter": "dfx deploy counter || exit 0;",
    "ic:generate:counter": "dfx generate counter || exit 0;",
    "ic:id": "dfx deploy internet_identity --no-wallet --argument '(null)' || exit 0;"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@chakra-ui/react": "^2.2.1",
    "@dfinity/agent": "0.10.0",
    "@dfinity/auth-client": "0.10.0",
    "@dfinity/authentication": "0.10.0",
    "@dfinity/candid": "0.10.0",
    "@dfinity/identity": "0.10.0",
    "@dfinity/principal": "0.10.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/inter": "^4.5.11",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.0",
    "classnames": "^2.3.1",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.11.3",
    "framer-motion": "^5.6.0",
    "graphql": "^16.5.0",
    "ramda": "^0.27.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.31.3",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.4.0",
    "twin.macro": "^2.8.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.0",
    "@types/node": "^16.11.39",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^7.0.4",
    "lint-staged": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vite-plugin-babel-macros": "^1.0.6"
  }
}
