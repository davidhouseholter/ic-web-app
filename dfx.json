{
  "canisters": {
    "counter": {
      "main": "service/counter.mo",
      "type": "motoko"
    },
    "access_hello": {
      "main": "service/access_hello.mo",
      "type": "motoko"
    },
    "ic_web_app": {
      "dependencies": [
        "counter"
      ],
      "frontend": {
        "entrypoint": "src/dist/index.html"
      },
      "source": [
        "public/",
        "dist/"
      ],
      "type": "assets"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "service/internet_identity.did",
      "wasm": "service/internet_identity.wasm",
  
      "_build": "curl -sSL https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm -o internet_identity.wasm"
    }
  },
  
  "defaults": {
    "build": {
      "args": "",
      "output": "build",
      "packtool": "vessel sources"
    }
  },
  "dfx": "0.10.0",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  
  "version": 1
}